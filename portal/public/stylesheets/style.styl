/* Style Design and Color Scheme
 =============================*/

 /* Master applies universally */
 $master-color = #F2F2F2
 $master-font-size = 14px
 $master-font-family = 'Roboto', sans-serif
 $master-font = $master-font-size $master-font-family

 /* Header relates to the top horizontal navigation bar */
 $header-height = 50px

 /* Navigation relates to the main left navigation bar */
 $navigation-width = 250px
 $navigation-height = 50px
 $navigation-width-collapsed = 40px
 $navigation-background = #3D3D3D
 $navigation-color = #F2F2F2
 $navigation-transition-width = 900px
 $navigation-hover-background = #545454
 $navigation-glyph-width = 45px
 $navigation-glyph-border = 9px
 $navigation-glyph-size = 1.7em
 $navigation-gutter = 20px

 /*Header background: #f4f4f4;
 Header foreground: #3D3D3D;
 Navigation background: #3D3D3D;
 Sidebar diminished text: #666666;

 =============================*/
/* Resets and Overrides
 =============================*/

@import "normalize.css"

*
  margin 0
  padding 0

/* Links and Type
 =============================*/

body
  font $master-font
  color $master-color

/* Layout
 =============================*/

/* Navigation Sidebar */

nav.navbar-side
    background $navigation-background
    width $navigation-width
    position fixed
    top 0
    left:0
    min-height 100%
    transition left .5s ease
    -webkit-transition left .5s ease
    -moz-transition left .5s ease
    z-index 9997

nav.navbar-side.collapsed
    left $navigation-width-collapsed - $navigation-width

@media only screen and (max-width: $navigation-transition-width)
    nav.navbar-side.collapsed
        left 0

    nav.navbar-side
        left $navigation-width-collapsed - $navigation-width

nav.navbar-side a
    text-decoration none
    color $navigation-color

.navbar-side ul li a
    display block
    height $navigation-height
    line-height $navigation-height
    padding-left $navigation-glyph-width
    position relative

.navbar-side ul li a:hover
    background $navigation-hover-background

.navbar-side ul li.active a
    background white
    border-left $navigation-gutter solid $navigation-background
    padding-left $navigation-glyph-width - $navigation-gutter
    color $navigation-background

.navbar-side ul li.active a:hover
    background white

.navbar-side ul li i
    position absolute
    height $navigation-height
    line-height $navigation-height
    right $navigation-glyph-border
    font-size $navigation-glyph-size

/* There can be a single paragraph in the side. */
.navbar-side p
    text-align center
    font-weight 100
    font-size .8em
    padding 0 40px
    color #666666
    position absolute
    bottom 10px

/* Fit the header over the top margin, bottom margin is space before navigation list. Padding provides the visible area. */
.navbar-header
    font-size 25px
    padding 15px
    margin 50px 0px 20px 0px

.navbar-header i
    position absolute
    right 11px
    top 58px
    cursor pointer

.navbar-top
    position fixed
    top 0
    width 100%
    background #f4f4f4
    border-bottom 1px solid #dddddd
    z-index 9998

.navbar-top img
    padding 15px
    height 20px
    float left
    transition margin-left .5s ease
    -webkit-transition margin-left .5s ease
    -moz-transition margin-left .5s ease

/* TODO: Is this needed? */
.navbar-top img.collapsed
    margin-left 40px

.navbar-top ul li
    list-style-type none
    margin-right 50px
    float right

@media only screen and (max-width: 768px)
    .navbar-top ul li
        margin-right 10px

.navbar-top ul li a
    text-decoration none
    color #3D3D3D
    line-height 50px

.navbar-top ul li a:hover
    color black

/* TODO: Is this needed? */
.flex-container
    overflow hidden

/* Main content styles */
.main-content {
    margin-left: $navigation-width
    margin-top:70px;
    padding:15px 30px;
    color:black;
    transition:margin-left .5s ease;
    -webkit-transition:margin-left .5s ease;
    -moz-transition:margin-left .5s ease;
}

.main-content.collapsed {
    margin-left:40px;
}

@media only screen and (max-width: 900px) {
    .main-content {
        margin-left:40px;
        margin-right:0;
        transition:all .5s ease;
        -moz-transition:all .5s ease;
        -webkit-transition:all .5s ease;
    }

    .main-content.collapsed {
        margin-right:-210px;
        margin-left:250px;
    }

    /* TODO: Is this needed? */
    .main-content.sidebar-inactive {
        margin-left:0;
    }
}

/* Main column header */
.page-header
    position relative
    padding-left 1%

.page-header i.fa-pencil
    background transparent
    color #D8D8D8
    font-size 20px

.page-header i.fa-pencil:hover
    background transparent
    color #c5c5c5

.page-header h1
    color #333
    font-weight 100
    font-size 30px
    font-variant small-caps

.page-header .back-btn
    position absolute
    top 11px
    left -25px
    transition all .25s ease
    -moz-transition all .25s ease
    -webkit-transition all .25s ease

@media only screen and (max-width: 768px) {
    .page-header h1 {
        font-size:2em;
    }

    .page-header .back-btn {
        top:0;
        left:3.1%;
    }
}

/* Main column page */
.page-text
    font-weight 300
    margin 25px 0

.page-text h1
    font-weight 100

.page-text p
    padding-left 5%

.page-text .btn
    margin-bottom 15px

/* Row styles */
.row
    padding-left 2%
    padding-right 2%
    width 96%
    overflow auto

.flexrow
    @extend .row
    display flex
    flex-wrap wrap

/* Generalized List */
.list-header {
    position:relative;
    height:40px;
    overflow:visible;
}
.list-header-group {
    overflow:auto;
    cursor:pointer;
}
.list-header span {
    padding-left:65px;
    font-style:italic;
    font-weight:100;
    float:left;
}
.list-header .dropdown-box-wrap {
    position: absolute;
    top: 0;
    right: 250px;
    border: 1px solid #ddd;
    height:30px;
    overflow:hidden;
    transition:all .25s ease;
}
.list-header ul {
    float: right;
    list-style-type: none;
    line-height: 30px;
    font-size:.9em;
    margin-left:1%;
    cursor:pointer;
    background: white;
    width:100%;
}
.list-header ul li {
    padding:0 25px 0 5px;
    transition:all .25s ease;
    -moz-transition:all .25s ease;
    -webkit-transition:all .25s ease;
}
.list-header ul li:hover {
    background:#e2e2e2;
}
.list-items {
    list-style-type: none;
}
.list-items:first-child {
    border-top:2px solid #F2F2F2;
}
.list-items li {
    padding:5px 0;
    border-bottom:2px solid #F2F2F2;
//    transition:height .25s ease;
//    -moz-transition:height .25s ease;
//    -webkit-transition:height .25s ease;
    overflow:hidden;
}
.list-items li h1 {
    font-size:1.5em;
}
.list-items li.display-info {
    height:90px;
}
.list-items li .badge-items {
    display: inline-block;
    font-size: 1em;
    font-weight: 100;
    float:right;
    margin-right:3%;
}
.list-items li .badge-items span {
    border-radius: 20px;
    display: inline-block;
    background: #E2E2E2;
    margin-top: 7px;
    padding: 0 10px;
    text-align: center;
}
.list-items li .badge-items span.tests-badge {
    color: white;
    background: #A0A0A0;
}
.list-items h1 {
    font-weight:300;
    float:left;
}
.list-items h2 {
    font-weight: 200;
    float: left;
}
.list-action-btns {
    float:right;
    margin-top:2px;
}
.list-action-btns i {
    font-size:1.6em;
    padding:4px 6px;
    border-radius:22px;
    transition:all .25s ease;
    -moz-transition:all .25s ease;
    -webkit-transition:all .25s ease;
}
.list-items.add-module li .list-action-btns {
    font-size:.8em;
}
.list-items.add-module li .list-action-btns i {
    font-size: 1.6em;
}
.list-items.add-module li .list-action-btns i.fa-tasks {
    font-size:1.3em;
}
.list-action-btns a {
    display:inline-block;
}
.list-items.add-module li {
    /*height:15px;*/
    transition:all .25s ease;
    -moz-transition:all .25s ease;
    -webkit-transition:all .25s ease;
}
.list-items.add-module li .badge-items span {
    margin-top:1px;
}
.list-items.add-module li .badge-items {
    font-size:1em;
}
.list-items.add-module h1 a {
    text-decoration:none;
    color:inherit;
}
.list-items.add-module h1 {
    font-size:1.3em;
}
.list-items.add-module li.not-selected {
    background:#f4f4f4;
    color:#6d6d6d;
    border-bottom:1px solid #d6d6d6;
}
.list-items li .item-info {
    float:left;
    width:100%;
    transition:all .25s ease;
    -moz-transition:all .25s ease;
    -webkit-transition:all .25s ease;
}
.list-items li .item-info.display-info {
    opacity:1;
}
.list-items li .item-info .description {
    background:#f4f4f4;
    padding:15px;
    font-style:italic;
    margin-top:10px;
}
.list-items li .item-info .script {
  background:#f4f4f4;
  padding:15px;
  font-family:monospace;
  margin-top:10px;
}

/* Report styles */
