/* Dashboard styles */
.dash-block {
    text-align:center;
    padding:15px;
    border:1px solid #f4f4f4;
    width: 120px;
    margin-left: 10px;
    margin-bottom: 10px;
}
.dash-block h2 {
    font-weight:300;
}
.dash-block p {
    font-weight:100;
    font-size:2.5em;
    border-top:1px solid rgb(163, 163, 163);
