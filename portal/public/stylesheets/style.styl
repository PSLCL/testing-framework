/* Create an item */
i.hdr-new
    transition:transform .25s ease;
    -moz-transition:-moz-transform .25s ease;
    -webkit-transition:-webkit-transform .25s ease;
i.display-create
    transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
.create-module-form, .create-plan-form, .create-test-form
    background:#F4F4F4;
    overflow:auto;
    padding:0 20px;
    height:10px;
    opacity:0;
    display:none;
    overflow:hidden;
    transition:all .25s ease;
    -moz-transition:all .25s ease;
    -webkit-transition:all .25s ease;
.display-block
    display:block !important;
.create-module-form.display-create, .create-plan-form.display-create, .create-test-form.display-create
    padding:30px 20px;
    height:20px;
    opacity:1;
.create-plan-form.display-create, .create-test-form.display-create
    height:175px
.create-module-form button, .create-test-form button, .create-plan-form button, .general-form button
    float:right;
    padding:8px 15px;
    color:white;
.create-module-form input, .create-test-form input, .create-plan-form input
    width:75%;
    border: 1px solid #ddd;
    margin-right:15px
.create-plan-form textarea, .create-test-form textarea
    width:75%;
    padding-left:5px;
    height:50px;
    border: 1px solid #ddd;
.create-module-form .help-block, .create-plan-form .help-block,
.general-form .help-block, .create-test-form .help-block
    color:#D30724;
/* adding a module switch styles */
.switch-background
    width: 58px;
    background: rgb(213, 213, 213);
    height: 26px;
    float: right;
    border-radius: 15px;
    position: relative;
    box-shadow:inset 1px 1px 1px rgba(0,0,0,.05);
    -moz-box-shadow:inset 1px 1px 1px rgba(0,0,0,.05);
    -webkit-box-shadow:inset 1px 1px 1px rgba(0,0,0,.05);
.switch-control
    position: absolute;
    height: 22px;
    background:#f4f4f4;
    width: 22px;
    border-radius: 20px;
    top: 1px;
    transition: all 0.25s ease;
    -moz-transition: all 0.25s ease;
    -webkit-transition: all 0.25s ease;
    box-shadow:1px 1px 1px rgba(0,0,0,.25)
    -moz-box-shadow:1px 1px 1px rgba(0,0,0,.25)
    -webkit-box-shadow:1px 1px 1px rgba(0,0,0,.25)
    z-index:1;
.switch-left
    position: absolute;
    width: 15px;
    height: 15px;
    background: rgb(99, 190, 99);
    border-radius: 15px;
    top: 6px;
    left: 5px;
.switch-background.selected .switch-control
    right:2px;
.switch-background.not-selected .switch-control
    right:34px;
