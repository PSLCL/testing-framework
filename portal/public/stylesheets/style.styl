    display:block;
}
.reports-section select {
    width:100%;
    height:150px;
}
/* results area */
.report-results {
    float:left;
    width:100%;
}
.results-header {
    margin-bottom:25px;
}
.results-header h2 {
    margin-bottom:10px;
}
.report-results h3 {
    color: #000;
}
.report-results h3 span {
    font-weight:100;
    font-style:italic;
}
.result-test-list {
    list-style-type: none;
    margin:5px 0 5px 3%;
}
.result-test-plan {
    margin:0 2%;
}
.result-test-plan h3{
    margin: 15px 0 5px;
    border-bottom: 1px solid black;
}
.result-test-list li {
    margin-bottom:5px;
}
.report-results table {
    width:100%;
}
.report-results table tbody tr {
    outline:1px solid rgb(204, 204, 204);
    background:white;
}
.report-results table td, .report-results table th {
    padding:2px 5px;
}

.hash {
    background:rgb(206, 206, 206);
    display:inline-block;
    padding:5px 10px;
    border-radius:10px;
    font-size:.9em;
    max-width: 94%;
    overflow-x: scroll;
}

/* Login Page styles */
.alert {
    text-align: center;
    background: rgb(214, 70, 70);
    padding: 10px 0;
    color: white;
}
.login-form {
    height:250px;
    padding:25px 0;
    text-align:center;
}
.login-form h3 {
    margin-bottom:20px;
}
.login-form input {
    display:block;
    width:70%;
    margin:15px auto;
}
.login-form button {
    margin:20px auto 0;
}

/* inline editing */
.general-form {
    background:transparent;
    padding:0;
}
.general-form input {
    width: auto;
    height: auto;
    font-size: 35px;
    font-weight:100;
}
.general-form textarea {
    width: 98%;
    height: auto;
    padding: 20px 5px;
    font-size: 1.17em;
    font-weight:100;
}
@media only screen and (max-width: 860px) {

}
@media only screen and (max-width: 768px) {
    .col-4 {
        width:96%;
        margin:0 2%;
    }
    .main-content {
        padding:0;
    }
    .general-form input {
        font-size: 2em;
    }
    .form-control.search {
        width:100%;
        position:relative;
        float:left;
        margin:20px 0 0;
        right:0;
    }
    .list-header {
        height:80px;
    }
    .list-header span {
        padding-left:46px;
    }
    .list-header .dropdown-box-wrap {
        right:.9%;
        top:-7px;
        z-index:9997;
    }
    .list-items.add-module li {
        overflow:hidden;
    }
    .list-items li.display-info {
        height:180px;
    }
    .list-items li h1 {
        width:100%;
    }
    .list-action-btns i {
        font-size: 1.6em;
    }
    .list-action-btns i.fa-tasks {
        font-size:1.3em;
    }
    .list-items li .badge-items {
        float:left;
    }
    .create-plan-form.display-create, .create-test-form.display-create {
        height:150px;
    }
    .create-module-form.display-create {
        height:80px;
    }
    .create-module-form .help-block, .create-plan-form .help-block, .general-form .help-block, .create-test-form .help-block {
        display:block;
    }

}
@media only screen and (max-width: 550px) {
    .box-left, .box-right {
        width:100%;
    }
    .move-arrows {
        width:100%;
        padding-top:0;
    }
    .move-arrows i {
        display:inline-block;
        transform: rotate(90deg);
        -moz-transform: rotate(90deg);
        -webkit-transform: rotate(90deg);
    }
    .report-results thead, .report-results tbody, .report-results td, .report-results th {
        display:block;
    }
    .report-results thead {
        width:30%;
    }
    .report-results tbody {
        width:70%
    }
    .report-results thead, .report-results tbody {
        float:left;
    }
    .report-results td, .report-results th {
        font-weight: 500;
        font-size: .8em;
    }
    .report-results th {
        border-bottom:1px solid #f4f4f4;
    }
    .report-results td {
        border-bottom:1px solid #cccccc;
    }
}
@media only screen and (max-width: 480px) {
    input, textarea {
        width:100% !important;
    }
    .reports-nav li a {
        height:35px;
    }
}
