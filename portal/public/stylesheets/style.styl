    float left

.report-module div.legend
    margin-left 110px

.report-module .legend p.total
    font-weight bold

.report-module .legend p.pass
    color #00CC00

.report-module .legend p.fail
    color #FF0000

.report-module .legend p.pending
    color #A0A0A0

.report-module .legend .total span.count::before
    content "Total: "

.report-module .legend .pass span.count::before
    content "Pass: "

.report-module .legend .fail span.count::before
    content "Fail: "

.report-module .legend .pending span.count::before
    content "Pending: "

.report-module .legend span.percent::before
    content " ("

.report-module .legend span.percent::after
    content "%)"

.results p.total
    font-weight bold

.results p.pass
    color #00CC00

.results p.fail
    color #FF0000
    word-wrap: break-word;

.results p.pending
    color #A0A0A0

.results .total span.count::before
    content "Total: "

.results .pass span.count::before
    content "Pass: "

.results .fail span.count::before
    content "Fail: "

.results .pending span.count::before
    content "Pending: "

.results span.percent::before
    content " ("

.results span.percent::after
    content "%)"

.report-module ul.list-module
    margin 0 0 0 0
    padding 0
    list-style-type none
    text-align left

.report-module .list-module li
    display inline-block
    margin 0 5px 5px 0

.module-list li h1
    font-size 1.5em

.module-list .organization
    color green

.module-list .module
    color blue

.module-list .attributes
    color purple

.module-list .version
    color gray

.module-list .sequence
    color blue

.module-list li .badge-items span
    border-radius 20px
    display inline-block
    background #E2E2E2
    margin-top 7px
    padding 0 10px
    text-align center

.module-list li .badge-items span.plans-badge
    color white
    background #A0A0A0

.module-list li .badge-items span.plans-badge::before
    content "Plans: "

.module-list li .badge-items span.tests-badge
    color white
    background #A0A0A0

.module-list li .badge-items span.tests-badge::before
    content "Tests: "

.test-plan-list li .badge-items span.tests-badge::before
    content "Tests: "

.artifact-list li .badge-items span.plans-badge::before
    content "Plans: "

.artifact-list li .badge-items span.tests-badge::before
    content "Tests: "


